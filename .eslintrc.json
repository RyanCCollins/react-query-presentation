{
    "plugins": ["react", "prettier"],
    "extends": ["react-app", "airbnb", "prettier"],
    "rules": {
      "import/prefer-default-export": "off",
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true
        }
      ],
      "react/jsx-filename-extension": [
        "warn",
        {
          "extensions": [".js", ".jsx"]
        }
      ]
    }
  }